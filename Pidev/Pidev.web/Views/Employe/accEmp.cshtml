@model IEnumerable<Pidev.data.tp_jsf_employe>
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="card-body">
    <div class="table-responsive">
        <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
            <thead>
                <tr>
                    <th>Email</th>
                    <th>IsActif?</th>
                    <th>Nom</th>
                    <th>Prenom</th>
                    <th>Role</th>
                    <th>Action</th>
                </tr>
            </thead>
            <tfoot>
                <tr>
                    <th>Email</th>
                    <th>IsActif?</th>
                    <th>Nom</th>
                    <th>Prenom</th>
                    <th>Role</th>
                    <th>Action</th>
                </tr>
            </tfoot>
            <tbody>
                @foreach (var i in ViewBag.result)
                {
                <tr>
                    <td>@i.email</td>
                    <td>@i.isActif</td>
                    <td>@i.nom</td>
                    <td>@i.prenom</td>
                    <td>@i.role</td>
                    <td>
                        <form action="del">
                            <button class="btn btn-google btn-user btn-block">DELETE</button>
                        </form>

                    </td>
                    
                </tr>
                }



            </tbody>
        </table>
    </div>
</div>